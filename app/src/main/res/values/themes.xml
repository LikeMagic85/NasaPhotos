<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NasaPhotos" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#160036</item>
        <item name="colorPrimaryVariant">#160036</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#35520c</item>
        <item name="colorSecondaryVariant">#35520c</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="chipStyle">@style/MyChipStyle</item>
        <item name="bottomSheetStyle">@style/MyBottomSheetStyle</item>
        <item name="textInputStyle">@style/MyEditTextStyle</item>
    </style>

</resources>